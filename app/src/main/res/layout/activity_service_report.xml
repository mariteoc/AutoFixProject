<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ServiceReport">

    <EditText
        android:id="@+id/editAppNumber"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="11dp"
        android:ems="10"
        android:hint="@string/hintAppNumber"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/txtServiceProv"
        app:layout_constraintEnd_toStartOf="@+id/btnReportGo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/btnReportGo"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginEnd="53dp"
        android:background="@drawable/button_bg"
        android:text="@string/btnGo"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/txtOutServProv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editAppNumber"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="53dp"
        android:layout_marginEnd="93dp"
        android:text="@string/txtEmail"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtOutEmail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPhone"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtServiceProv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="36dp"
        android:text="@string/txtServiceProv"
        app:layout_constraintBottom_toTopOf="@+id/txtCity"
        app:layout_constraintEnd_toStartOf="@+id/txtOutServProv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editAppNumber"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtCity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="114dp"
        android:text="@string/txtCity"
        app:layout_constraintBottom_toTopOf="@+id/txtServiceExec"
        app:layout_constraintEnd_toStartOf="@+id/txtOutCity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtServiceProv"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtServiceExec"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="32dp"
        android:text="@string/txtServiceExec"
        app:layout_constraintBottom_toTopOf="@+id/txtDate"
        app:layout_constraintEnd_toStartOf="@+id/txtOutServExec"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCity"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="109dp"
        android:text="@string/txtDate"
        app:layout_constraintBottom_toTopOf="@+id/txtTime"
        app:layout_constraintEnd_toStartOf="@+id/txtOutDate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtServiceExec"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="105dp"
        android:text="@string/txtTime1"
        app:layout_constraintBottom_toTopOf="@+id/txtCustomer"
        app:layout_constraintEnd_toStartOf="@+id/txtOutTime"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDate"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtCustomer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="53dp"
        android:layout_marginEnd="74dp"
        android:text="@string/txtCustomer"
        app:layout_constraintBottom_toTopOf="@+id/txtPhone"
        app:layout_constraintEnd_toStartOf="@+id/txtOutCustomer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTime"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="53dp"
        android:layout_marginEnd="94dp"
        android:text="@string/txtPhone"
        app:layout_constraintBottom_toTopOf="@+id/txtEmail"
        app:layout_constraintEnd_toStartOf="@+id/txtOutPhone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCustomer"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutPhone"
        android:layout_width="191dp"
        android:layout_height="29dp"
        android:layout_marginEnd="35dp"
        app:layout_constraintBottom_toTopOf="@+id/txtOutEmail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtPhone"
        app:layout_constraintTop_toBottomOf="@+id/txtOutCustomer"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutCustomer"
        android:layout_width="184dp"
        android:layout_height="29dp"
        android:layout_marginEnd="35dp"
        app:layout_constraintBottom_toTopOf="@+id/txtOutPhone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtCustomer"
        app:layout_constraintTop_toBottomOf="@+id/txtOutTime"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutTime"
        android:layout_width="184dp"
        android:layout_height="29dp"
        android:layout_marginEnd="41dp"
        app:layout_constraintBottom_toTopOf="@+id/txtOutCustomer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtTime"
        app:layout_constraintTop_toBottomOf="@+id/txtOutDate"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutDate"
        android:layout_width="182dp"
        android:layout_height="29dp"
        android:layout_marginEnd="38dp"
        app:layout_constraintBottom_toTopOf="@+id/txtOutTime"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtDate"
        app:layout_constraintTop_toBottomOf="@+id/txtOutServExec"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutServExec"
        android:layout_width="186dp"
        android:layout_height="27dp"
        android:layout_marginEnd="41dp"
        app:layout_constraintBottom_toTopOf="@+id/txtOutDate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtServiceExec"
        app:layout_constraintTop_toBottomOf="@+id/txtOutCity"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutCity"
        android:layout_width="184dp"
        android:layout_height="29dp"
        android:layout_marginEnd="41dp"
        app:layout_constraintBottom_toTopOf="@+id/txtOutServExec"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtCity"
        app:layout_constraintTop_toBottomOf="@+id/txtOutServProv"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutServProv"
        android:layout_width="187dp"
        android:layout_height="31dp"
        android:layout_marginEnd="41dp"
        app:layout_constraintBottom_toTopOf="@+id/txtOutCity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtServiceProv"
        app:layout_constraintTop_toBottomOf="@+id/btnReportGo"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtOutEmail"
        android:layout_width="192dp"
        android:layout_height="26dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtEmail"
        app:layout_constraintTop_toBottomOf="@+id/txtOutPhone"
        app:layout_constraintVertical_bias="0.5" />

</androidx.constraintlayout.widget.ConstraintLayout>